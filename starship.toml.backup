format = "[](#89b4fa)$username[](bg:#cba6f7 fg:#89b4fa)$directory[](fg:#cba6f7 bg:#1e1e2e)$git_branch$git_status[](fg:#1e1e2e bg:#89b4fa)$c$elixir$elm$golang$gradle$haskell$java$julia$nodejs$nim$rust$scala[](fg:#89b4fa)"

[username]
show_always = true
style_user = "bg:#89b4fa fg:#11111b"
style_root = "bg:#89b4fa fg:#11111b"
format = '[ $user ]($style)'

[directory]
style = "bg:#cba6f7 fg:#11111b"
format = "[ $path ]($style)"
truncation_length = 3
truncation_symbol = "…/"

[directory.substitutions]
"Documents" = "󰈙 "
"Downloads" = " "
"Music" = " "
"Pictures" = " "

[git_branch]
symbol = ""
style = "bg:#1e1e2e"
format = '[[ $symbol $branch ](fg:#cba6f7 bg:#1e1e2e)]($style)'

[git_status]
style = "bg:#1e1e2e"
format = '[[($all_status$ahead_behind )](fg:#cba6f7 bg:#1e1e2e)]($style)'

[nodejs]
symbol = ""
style = "bg:#89b4fa"
format = '[[ $symbol ($version) ](fg:#11111b bg:#89b4fa)]($style)'

[rust]
symbol = ""
style = "bg:#89b4fa"
format = '[[ $symbol ($version) ](fg:#11111b bg:#89b4fa)]($style)'

[golang]
symbol = ""
style = "bg:#89b4fa"
format = '[[ $symbol ($version) ](fg:#11111b bg:#89b4fa)]($style)'

[java]
symbol = ""
style = "bg:#89b4fa"
format = '[[ $symbol ($version) ](fg:#11111b bg:#89b4fa)]($style)'

[php]
symbol = ""
style = "bg:#89b4fa"
format = '[[ $symbol ($version) ](fg:#11111b bg:#89b4fa)]($style)'

[time]
disabled = false
time_format = "%R" # Hour:Minute Format
style = "bg:#1e1e2e"
format = '[[  $time ](fg:#89b4fa bg:#1e1e2e)]($style)'
[python]
symbol = " "
style = "bg:#89b4fa"
format = '[[ $symbol ($virtualenv) ](fg:#11111b bg:#89b4fa)]($style)'
detect_extensions = ['py']
detect_files = ['.python-version', 'Pipfile', '__pycache__', 'pyproject.toml', 'requirements.txt', 'setup.py', 'tox.ini']
detect_folders = []
pyenv_version_name = false
pyenv_prefix = 'pyenv '
python_binary = ['python', 'python3', 'python2']
